#################### 
##### Debug ######## 
#################### 

# NOTE:  If in non-login shell, then this file is called first instead of bash_profile.  Therefore 
#        bash_profile must be sourced and bash_profile must not source bashrc if non-login shell
if [ ${0:0:1} != '-'  ] ; then 
    if [ -f "$HOME/.bash_profile" ]; then
        if [[ "$bash_debug" == 'yes' ]]; then 
            printf '\e[38;5;37m\nsourcing bashrc bc Non-Login \e[0m'
        fi 
        source $HOME/.bash_profile
    fi

    if [ -f "$HOME/.bash_aliases" ]; then
        if [[ "$bash_debug" == 'yes' ]]; then 
            printf '\e[38;5;37m\nsourcing bash_aliases bc Non-Login \e[0m'
        fi 
        source $HOME/.bash_aliases
    fi
fi

if [[ "$bash_debug" == 'yes' ]]; then 
  printf '\e[38;5;26m\nbashrc end\e[0m\n'
fi 
