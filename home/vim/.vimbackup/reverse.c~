#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define MAX 4


char * reverse(char word[],int k, int top);
char * swap (char ** word){
  char  * tmp = *word ;
  *word = *(word+1);
  *(word+1) = tmp ;
  return *word ;
}
int main(){
  char * name = (char *)malloc(sizeof(char)*20);
  char * name2 = (char *)malloc(sizeof(char)*20);
  scanf("%s",name);
  name2 = swap(&name);
  printf("%s\n",name);
}
