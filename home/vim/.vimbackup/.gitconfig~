  log-hist = !git log-pretty --graph
  log-pretty = log --pretty='format:%C(blue)%h%C(red)%d%C(yellow) %s %C(green)%an%Creset, %ar'
  l = !git log-hist
  log-hist = !git log-pretty --graph
  log               = The log command
  n = !"git ls-files | xargs notes | awk -F: '{ print $1,$2; print $3,$4; print $5}' | grcat onf.notes"
[git-up "bundler"]
  check = true
[init]
  templatedir = ~/.git_template
